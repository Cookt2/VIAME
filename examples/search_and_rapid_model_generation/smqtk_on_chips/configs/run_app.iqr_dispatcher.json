{
    "flask_app": {
        "BASIC_AUTH_PASSWORD": "demo",
        "BASIC_AUTH_USERNAME": "demo",
        "SECRET_KEY": "MySuperUltraSecret"
    },
    "iqr_tabs" :
        {
        "Data-set Iqr": {
            "name": "data-set",
            "data_set": {
		"DataMemorySet": {
		    "cache_element": {
		        "DataFileElement": {
		            "explicit_mimetype": null,
		            "filepath": "models/image_elements.dms_cache",
		            "readonly": false
		        },
		        "type": "DataFileElement"
		    },
		    "pickle_protocol": -1
		},
		"type": "DataMemorySet"
            },
            "descr_generator": {
                "CaffeDescriptorGenerator": {
                    "batch_size": 100,
                    "data_layer": "data",
                    "image_mean_uri": "../../common_data/models/default_caffe_mean.binaryproto",
                    "input_scale": null,
                    "load_truncated_images": true,
                    "network_is_bgr": true,
                    "network_model_uri": "../../common_data/models/default_caffe_clfr.caffemodel",
                    "network_prototxt_uri": "../../common_data/models/default_caffe_clfr.prototxt",
                    "pixel_rescale": null,
                    "return_layer": "fc7",
                    "use_gpu": false
                },
                "type": "CaffeDescriptorGenerator"
            },
            "descriptor_factory": {
                "DescriptorFileElement": {
                    "save_dir": "models/descriptors",
                    "subdir_split": null
                },
                "PostgresDescriptorElement": {
                    "binary_col": "vector",
                    "db_host": "127.0.0.1",
                    "db_name": "postgres",
                    "db_pass": null,
                    "db_port": 5432,
                    "db_user": "smqtk",
                    "table_name": "descriptors_alexnet_fc7",
                    "type_col": "type_str",
                    "uuid_col": "uid"
                },
                "type": "DescriptorFileElement"
            },
            "nn_index": {
                "LSHNearestNeighborIndex": {
		    "descriptor_index": {
			"PostgresDescriptorIndex": {
			    "create_table": false,
			    "db_host": "127.0.0.1",
			    "db_name": "postgres",
			    "db_pass": null,
			    "db_port": 5432,
			    "db_user": "postgres",
			    "element_col": "element",
			    "multiquery_batch_size": 1000,
			    "pickle_protocol": -1,
			    "read_only": false,
			    "table_name": "descriptor_index_alexnet_fc7",
			    "uuid_col": "uid"
			},
			"type": "PostgresDescriptorIndex"
		    },
                    "distance_method": "cosine",
		    "hash2uuids_kvstore": {
			"MemoryKeyValueStore": {
			    "cache_element": {
				"DataFileElement": {
				    "explicit_mimetype": null,
				    "filepath": "models/hash2uuids.memKvStore.pickle",
				    "readonly": true
				},
				"type": "DataFileElement"
			    }
			},
			"type": "MemoryKeyValueStore"
		    },
                    "lsh_functor": {
                        "ItqFunctor": {
                            "bit_length": 256,
                            "itq_iterations": 100,
                            "mean_vec_cache": {
                                "DataFileElement": {
                                    "explicit_mimetype": null,
                                    "filepath": "models/itq.model.b256_i100_r0.mean_vec.npy"
                                },
                            "type": "DataFileElement"
                            },
                            "normalize": null,
                            "random_seed": 0,
                            "rotation_cache": {
                                "DataFileElement": {
                                    "explicit_mimetype": null,
                                    "filepath": "models/itq.model.b256_i100_r0.rotation.npy"
                                },
                            "type": "DataFileElement"
                            }
                        },
                        "type": "ItqFunctor"
                    },
                    "read_only": false
                },
                "type": "LSHNearestNeighborIndex"
            },
            "pos_seed_neighbors": 500,
            "rel_index_config": {
                "LibSvmHikRelevancyIndex": {
                    "descr_cache_filepath": null
                },
                "type": "LibSvmHikRelevancyIndex"
            },
            "url_prefix": "fish",
            "working_directory": "models/iqr_app_work"
        }
    },
    "mongo": {
        "database": "smqtk",
        "server": "127.0.0.1:27017"
    },
    "server": {
        "host": "localhost",
        "port": 5000
    }
